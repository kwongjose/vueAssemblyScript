{
  "name": "assemblyscript-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": " npm run asbuild && npm run placeAsm && vue-cli-service serve",
    "build": " npm run asbuild && npm run placeAsm && vue-cli-service build",
    "placeAsm": "cp ./build/optimized.wasm ./public/wasm && cp ./build/optimized.wasm.map ./public/wasm",
    "lint": "vue-cli-service lint",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap http://localhost:8080/untouched.wasm.map --validate --debug",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap http://localhost:8080/wasm/optimized.wasm.map --validate --debug --optimize -O3s",
    "asbuild": "npm run asbuild:optimized"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.8.1",
    "assemblyscript": "^0.8.1-nightly.20200120",
    "benchmark": "^2.1.4",
    "core-js": "^3.4.4",
    "typescript": "^3.7.5",
    "vue": "^2.6.10",
    "vue-wasm": "^1.0.0",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10",
    "@vue/cli": "^4.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
